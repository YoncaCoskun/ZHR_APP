{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "ZHR_CELEBI",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "ZHR_CELEBI.view.App",
			"type": "XML"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "ZHR_CELEBI.view",
				"controlId": "rootControl",
				"controlAggregation": "pages",
				"viewType": "XML"
			},
			"routes": [{
				"name": "screen1",
				"pattern": "",
				"target": "screen1"
			}, {
				"name": "screen2",
				"pattern": "Screen2",
				"target": "screen2"
			}],
			"targets": {
				"screen1": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Screen1"
				},
				"screen2": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Screen2",
					"viewLevel": 1
				}
			}
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ZHR_CELEBI.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}